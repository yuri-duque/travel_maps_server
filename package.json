{
  "name": "travel_maps_server",
  "version": "0.1.0",
  "description": "server to travel maps app",
  "main": "src/server.ts",
  "scripts": {
    "dev": "yarn swagger & nodemon src/server.ts",
    "start": "ts-node src/server.ts",
    "build": "yarn swagger && tsc --outDir public",
    "deploy": "node public/src/server.js",
    "swagger": "node swagger",
    "swagger:commit": "git add ./swagger/swagger_output.json",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "dependencies": {
    "async": "^3.2.4",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.1",
    "mongoose": "^7.3.2",
    "nodemon": "^3.0.1",
    "swagger-autogen": "^2.23.4",
    "swagger-ui-express": "^5.0.0",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/async": "^3.2.20",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^18.14.0",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "eslint": "^8.53.0",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.3.1",
    "eslint-plugin-promise": "^6.1.1",
    "pre-commit": "^1.2.2",
    "prettier": "^3.1.0",
    "prettier-plugin-organize-imports": "^3.2.4",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=16.20.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yuri-duque/travel_maps_server.git"
  },
  "keywords": [
    "maps",
